<%= form_for @students, :url=>{:controller=>'students',:action => 'update'} do |f| %>

<div class="field">
    <%= f.label :username %><br />
    <%= f.text_field :username %>
  </div>
  <div class="field">
    <%= f.label :password%><br />
    <%= f.password_field :password %>
  </div>
<div class="actions">
	
    <%= f.submit 'create' %>
  </div>
<% end %>

<%= link_to 'back', :action=>'index'%>
